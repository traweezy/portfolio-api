datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model project {
  id          Int     @id @default(autoincrement())
  name        String
  description String
  image       String?
}

model user {
  id       String  @id
  email    String  @unique
  password String
  token    String?
}
