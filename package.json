{
  "name": "portfolio-api",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "npm run tsc && cp -r src/public dist/src/",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start:dev": "tsnd --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "postinstall": "npm run build",
    "start": "node ./dist/src/index.js",
    "test": "npm run test:lint && npm run test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "npm run test:unit",
    "test:lint": "eslint src --ext .ts",
    "test:lint:fix": "eslint src --ext .ts --fix",
    "prettier": "prettier '{src,test}/**/*.ts' --write",
    "security-audit": "npm audit --only=prod"
  },
  "dependencies": {
    "@tsed/ajv": "^6.53.0",
    "@tsed/common": "^6.53.0",
    "@tsed/core": "^6.53.0",
    "@tsed/di": "^6.53.0",
    "@tsed/exceptions": "^6.53.0",
    "@tsed/json-mapper": "^6.53.0",
    "@tsed/passport": "^6.53.0",
    "@tsed/platform-express": "^6.53.0",
    "@tsed/schema": "^6.53.0",
    "@tsed/swagger": "^6.53.0",
    "ajv": "7.2.4",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "deep-email-validator": "^0.1.18",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-favicon": "^2.0.1",
    "method-override": "^3.0.0",
    "nodemailer": "^6.6.1",
    "passport": "^0.4.1"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "3.1.2",
    "@tsed/cli-plugin-jest": "3.1.2",
    "@tsed/cli-plugin-passport": "3.1.2",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/method-override": "^0.0.31",
    "@types/multer": "^1.4.5",
    "@types/node": "^15.12.1",
    "@types/nodemailer": "^6.4.2",
    "@types/passport": "^1.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^31.0.0",
    "jest": "^27.0.4",
    "prettier": "^2.3.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.2",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.2"
  },
  "tsed": {
    "packageManager": "npm",
    "convention": "default"
  },
  "engines": {
    "node": "16.3.0"
  }
}
